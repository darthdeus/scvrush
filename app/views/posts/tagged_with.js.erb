$(".active").removeClass("active");
$(".tagged_with-submit").attr("href", "#");
$(".tagged-content").fadeOut("slow", function() {
	$(".tagged-content").html(" ");

	<% @posts.each do |post| %>
    $(".tagged-content").append("<%= escaped_post_content(post) %>");
	<% end %>

	$(".tagged-content").fadeIn("slow");
})
